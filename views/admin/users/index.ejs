
<%- include('../../partials/header.ejs') %>
<%- include('../../partials/navbar.ejs') %>
      

<div class="container">
    <h2 class="mt-3">Usuarios</h2>
    <a href="/admin/user/create" class="btn btn-success mt-3">Criar Novo Usuario</a>
    <table class="table table-bordered mt-5">
        <thead>
            <tr>
                <th>ID</th>
                <th>NOME</th>
                <th>Senha</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <% users.forEach(user => { %>
                <td><%= user.id %></td>
                <td><%= user.email %></td>
                <td>*****</td>
                <td>
                    <button class="btn btn-warning">Editar</button>
                    <button class="btn btn-danger">Excluir</button>
                </td>
                <% }) %>
            </tr>
        </tbody>
    </table>
        </div>
    </div>
</div>

    
   


<%- include('../../partials/footer.ejs') %></div>